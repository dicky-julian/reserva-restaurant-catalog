{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": 0,
        "consistent-return": 0,
        "linebreak-style": 0,
        "import/prefer-default-export": 0,
        "no-console": 0,
        "no-mixed-operators": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-return-assign": 0,
        "no-restricted-globals": 0,
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0
    }
}
